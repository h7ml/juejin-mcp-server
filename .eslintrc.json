{
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "prefer-const": "error",
    "no-var": "error",
    "no-undef": "off"
  },
  "env": {
    "node": true,
    "es2022": true
  },
  "ignorePatterns": [
    "dist/**",
    "node_modules/**",
    "*.d.ts"
  ]
}
